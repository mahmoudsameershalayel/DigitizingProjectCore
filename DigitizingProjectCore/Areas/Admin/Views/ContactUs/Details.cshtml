@model DigitizingProjectCore.Areas.Admin.Dto.CreateContactUsDto
@{
    Layout = null;
}
<form action="/Admin/ContactUs/Details" class="ajaxForm" method="POST">
    <div class="form-horizontal">
        <div class="form-group">
            <label class="control-label col-md-2">Subject</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="Subject" readonly />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">Name</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="Name" readonly />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">Email</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" asp-for="Email" readonly />
            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-2">Message</label>
            <div class="col-sm-10">
                <textarea class="form-control" cols="20" id="Message" readonly="True" rows="15" asp-for="Message"></textarea>
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <a class="btn btn-default" data-dismiss="modal" href="/Admin/ContactUs/Index">Close</a>
            </div>
        </div>
    </div>
</form>