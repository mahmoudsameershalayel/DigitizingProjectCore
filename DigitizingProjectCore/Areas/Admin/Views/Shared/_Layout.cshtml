    <style>
        .lblBalance {
            font-size: 15px !important;
            display: inline-block;
            margin-top: 6px;
        }

        .table .dropdown-menu {
            left: auto !important;
            right: 0px !important;
        }

        table.dataTable thead .sorting:after, table.dataTable thead .sorting_asc:after, table.dataTable thead .sorting_desc:after, table.dataTable thead .sorting_asc_disabled:after, table.dataTable thead .sorting_desc_disabled:after {
            right: 14px;
        }

        .table-scrollable .dataTable td > .btn-group, .table-scrollable .dataTable th > .btn-group {
            right: 30px;
        }

        .select2-container {
            width: 100% !important;
        }

        .imgGallery li {
            display: inline-block;
            position: relative;
            width: 170px;
            overflow: hidden;
        }

            .imgGallery li img {
                height: 170px;
                margin-bottom: 5px;
                display: block;
            }

        .mRed {
            color: #b11d51;
        }

        .mBlue {
            color: #07a0b1;
        }

        .rTable thead tr {
            background-color: #b11d51;
            color: #fff;
        }

        .rTable tbody tr:nth-child(2n) {
            background-color: #ffcedf;
        }

        .bTable thead tr {
            background-color: #07a0b1;
            color: #fff;
        }

        .bTable tbody tr:nth-child(2n) {
            background-color: #89edf8;
        }

        .gTable thead tr {
            background-color: #01b43c;
            color: #fff;
        }

        .gTable tbody tr:nth-child(2n) {
            background-color: #8bf8af;
        }

        .centerAll table td, .centerAll table th {
            text-align: center !important;
        }

        .centerAll .table {
            margin-bottom: 0px !important
        }

        .by {
            color: #c6cfda;
            display: block;
            float: right;
            padding-top: 3px;
            padding-left: 373px;
            text-align: center;
        }

        @@media screen and (max-width:780px) {
            .by {
                width: 100%;
                padding-left: 0px;
            }
        }
    </style>
    <style>
        .dataTables_length {
            display: none;
        }

        a {
            color: #000000;
        }

        .redColor {
            color: red;
        }

        [type=number] {
            font-family: 'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
        }

        body, h1, h2, h3, h4, h5, h6, .dropdown-menu, .page-title, .select2-container--bootstrap .select2-selection {
            font-family: 'Noto Kufi Arabic', sans-serif;
            font-weight: bold;
        }

        .control-label, label {
            font-weight: bold;
        }

