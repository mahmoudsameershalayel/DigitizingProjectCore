    <script>

        toastr.options = {
            "closeButton": true,
            "debug": false,
            "positionClass": "toast-bottom-right",
            "onclick": null,
            "showDuration": "1000",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
        function UpdateDeleteSelectedButton() {

        }
        var msgTitle = "@ViewBag.Title";
        function ShowMssage(msg) {
            var cls = "info";
            if (msg.indexOf("s:") == 0) { cls = "success"; msg = msg.substring(2); }
            if (msg.indexOf("w:") == 0) { cls = "warning"; msg = msg.substring(2); }
            if (msg.indexOf("e:") == 0) { cls = "error"; msg = msg.substring(2); }
            if (msg.indexOf("i:") == 0) { cls = "info"; msg = msg.substring(2); }
            toastr[cls](msg, msgTitle);
        }
        $(function () {
            PageLoadActions();

            $(document).on("click", ".Confirm", function () {
                //اظهر الموديل
                $("#Confirm").modal("show");
                //
                $("#Confirm .btn-danger").attr("href", $(this).attr("href"));
                //ما تكملش
                return false;
            });

            $(document).on("click", ".PopUp", function () {
                $("#PopUp").modal({ show: true, backdrop: "static" });
                $("#PopUp .modal-body").html('<h3 class="text-center"  style="color:#36c6d3 !important"><i class="fa fa-spinner fa-spin fa-3x fa-fw"></i></h3>');
                $("#PopUp .modal-body").load($(this).attr("href"), function () {
                    setTimeout(function () { $("#PopUp .modal-body [autofocus]").focus().select(); }, 500);
                });
                $("#PopUp .modal-title").text($(this).attr("title"));
                return false;
            });

            $(document).on("click", ".PopUp2", function () {
                $("#PopUp2").modal({ show: true, backdrop: "static" });
                $("#PopUp2 .modal-body").html('<h3 class="text-center"  style="color:#36c6d3 !important"><i class="fa fa-spinner fa-spin fa-3x fa-fw"></i></h3>');
                $("#PopUp2 .modal-body").load($(this).attr("href"), function () {
                    setTimeout(function () { $("#PopUp2 .modal-body [autofocus]").focus().select(); }, 500);
                });
                $("#PopUp2 .modal-title").text($(this).attr("title"));
                return false;
            });
            $(document).on("click", ".PopUp3", function () {
                $("#PopUp3").modal({ show: true, backdrop: "static" });
                $("#PopUp3 .modal-body").html('<h3 class="text-center"  style="color:#36c6d3 !important"><i class="fa fa-spinner fa-spin fa-3x fa-fw"></i></h3>');
                $("#PopUp3 .modal-body").load($(this).attr("href"), function () {
                    setTimeout(function () { $("#PopUp3 .modal-body [autofocus]").focus().select(); }, 500);
                });
                $("#PopUp3 .modal-title").text($(this).attr("title"));
                return false;
            });
            $(document).ajaxStart(function () {
                $(".wait").text("الرجاء الانتظار...");
                NProgress.start();
            });
            $(document).ajaxStop(function () {
                $(".wait").text("");
                NProgress.done();
            });
            $(document).ajaxError(function () {
                $(".wait").text("");
                NProgress.done();
                $(".ajaxForm :submit").prop("disabled", false);
            });
        });
        function PageLoadActions() {
            $(".timepicker").timepicker({ showMeridian: false });
            $("input[type=submit]").click(function () {
                $(".tab-content").find("div.tab-pane:hidden:has(span.text-danger)").each(function (index, tab) {
                    var id = $(tab).attr("id");
                    $('a[href="#' + id + '"]').tab('show');
                });
            });


            UploadFileHandler();
            $(".date").each(function () {
                if ($(this).attr("Full") == "true") {
                    $(".date").datepicker({ format: "yyyy/mm/dd" });
                }
                else {
                    $(".date").datepicker({ format: "yyyy/mm/dd", endDate: "+0d" });
                }
            })
            $(".input-daterange").datepicker({ format: "yyyy/mm/dd", endDate: "+0d", autoclose: true });

            if ($("#tblAjax").size() > 0) {
                $("#Confirm .btn-danger").click(function () {
                    var url = $(this).attr("href");
                    var rowsCount = $("#tblAjax tbody tr").size();
                    $.ajax({
                        url: url,
                        success: function (json) {
                            if (json.status == 1) {
                                BindDataTable();
                            }
                            ShowMssage(json.msg);
                        }
                    });
                    $("#Confirm").modal("hide");
                    return false;
                });
            }
            $(document).on("submit", ".DTForm", function () {
                BindDataTable();
                return false;
            });
            //$(".DTForm :text,.DTForm select").change(function () { $(".DTForm").submit(); });
            $(".form-control").each(function () {
                var type = $(this).attr("type");
                if ($(this).parent().prev().is(".control-label") && (type == "text" || type == "number" || type == "password" || type == "email"))
                    $(this).attr("placeholder", "Please Enter " + $(this).parent().prev().text());
            });
            $(".select2").each(function () {
                var placeholder = $(this).attr("placeholder");
                if (!placeholder) {
                    placeholder = "";
                }
                $(this).select2({
                    placeholder: placeholder
                });
            })
            $(".ajaxForm").ajaxForm({
                success: function (json) {
                    if (json.status == 1) {
                        if (!$("#tblItems").hasClass("autohide")) {
                            if (json.close != 1) {
                                $(".ajaxForm").resetForm();
                                $(".ajaxForm :selected").prop("selected", false);
                                $(".select2").trigger("change");
                            }
                            $("#tblItems tbody tr").remove();
                            $("#tblItems").addClass("hidden").next().show();
                            if ($("#tblAjax").size() > 0) {
                                BindDataTable();
                            }
                            if ($("#tblDetails").size() > 0) {
                                LoadDetails();
                            }
                            if (json.Id > 0 && json.Bind == "Bind1")
                                Bind1(json.Id);
                            if (json.Id > 0 && json.Bind == "Bind2")
                                Bind2(json.Id);
                        }
                        else {
                            angular.element('.ajaxForm').scope().resetModel();
                            $(".select2").val('').trigger("change");
                        }
                    }
                    if (json.msg != null)
                        ShowMssage(json.msg);
                    if (json.redirect != null)
                        window.location = json.redirect;
                    if (json.close == 1)
                        $("#PopUp").modal("hide");
                    $(".ajaxForm :submit").prop("disabled", false);
                },
                beforeSubmit: function () {
                    $(".ajaxForm :submit").prop("disabled", true);
                }
            });

        }

        function UploadFileHandler() {
            $(document).on("click", ".btnDeleteFile", function () {
                var cc = $(this);
                $.post("/CMS/Home/DeleteFile", { id: $(this).data("fname") }, function (json) {
                    var InputFileName = $(cc).closest("table").prev().find("[type=file]").data("hname");
                    $(cc).closest("table").prev().show();
                    $(cc).closest("table").remove();
                    $("#" + InputFileName).val("");
                    if (json.status == 1) ShowMssage(json.msg);
                });
            });
            $(".btnUpload").click(function () {
                if (window.FormData !== undefined) {
                    var fileUpload = $(this).prev().get(0);
                    var files = fileUpload.files;
                    var fileData = new FormData();
                    for (var i = 0; i < files.length; i++) {
                        fileData.append(files[i].name, files[i]);
                    }
                    var x = $(this);
                    var InputFileName = $(this).prev().data('hname');
                    fileData.append('type', $(this).prev().data('type'));
                    $.ajax({
                        url: '/CMS/Home/PostUploadFiles',
                        type: "POST",
                        contentType: false, // Not to set any content header
                        processData: false, // Not to process data
                        data: fileData,
                        success: function (result) {
                            if (result.status != 1) {
                                ShowMssage(result.msg);
                            }
                            else {
                                $(x).prev().val("");
                                ShowMssage(result.msg);
                                var html = "<table class='table table-condensed table-bordered'>";
                                $(result.images).each(function () {
                                    html += "<tr><td>" + this.oldfname +
                                        "</td><td width=7%><a data-fname='" + this.fname
                                        + "' class='btn btnDeleteFile btn-danger btn-xs'><i class='fa fa-trash'></i> Remove</a></td></tr>";
                                    if ($("#" + InputFileName).val() == "")
                                        $("#" + InputFileName).val(this.fname);
                                    else {
                                        if ($(x).prev().is("[multiple]")) {
                                            $("#" + InputFileName).val($("#" + InputFileName).val() + "," + this.fname);

                                        }
                                        else {
                                            $("#" + InputFileName).val(this.fname);
                                        }
                                    }
                                });
                                html += "</table>";
                                $(x).parent().after(html);
                                $(x).parent().hide();
                            }
                        },
                        error: function (err) {
                            alert(err.statusText);
                        }
                    });
                } else {
                    alert("FormData is not supported.");
                }
            });
        }
        //حتتسلم للصفحة الام
        function ToJavaScriptDate(value) {
            if (value == null || value == "" || value == undefined) return "";
            var pattern = /Date\(([^)]+)\)/;
            var results = pattern.exec(value);
            var dt = new Date(parseFloat(results[1]));
            return dt.getDate() + "/" + (dt.getMonth() + 1) + "/" + dt.getFullYear();
        }
