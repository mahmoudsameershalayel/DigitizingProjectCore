    <script>
        function htmlEncode(value) {
            //create a in-memory div, set it's inner text(which jQuery automatically encodes)
            //then grab the encoded contents back out.  The div never exists on the page.
            return $('<div/>').text(value).html();
        }
        window.onpopstate = function (e) {
            if (e.state) {
                $(".RenderBody").html('<br><br><br><br><br><br><h3 class="text-center" style="color:#36c6d3 !important"><i class="fa fa-spinner fa-spin fa-3x fa-fw"></i></h3>');
                $(".RenderBody").load(e.target.location.href);
                $(".PageTitle").html(e.state.pageTitle);
            }
        };
        $(document).ready(function () {
            $(document).on("click", ".btnRemovePartner", function () {
                if (confirm("Are you sure?")) {
                    $(this).closest("tr").next().remove();
                    $(this).closest("tr").remove();
                }
