@model DigitizingProjectCore.Areas.Admin.Dto.CreateUpdateBrandDto
@{
}
<form asp-controller="Brand" asp-action="Add" method="POST">
    <div class="form-group">
        <label for="nameEn">Name (English)</label>
        <input type="text" class="form-control" id="nameEn" asp-for="NameEn">
    </div>
    <div class="form-group">
        <label for="nameAr">Name (Arabic)</label>
        <input type="text" class="form-control" id="nameAr" asp-for="NameAr">
    </div>
    <div class="form-group">
        <label for="url">URL</label>
        <input type="text" class="form-control" id="url" asp-for="URL">
    </div>
    <div class="form-group">
        <label for="active">Active</label>
        <input type="checkbox" id="active" checked asp-for="IsActive">
    </div>
    <div class="form-group">
        <label for="active">Partner</label>
        <input type="checkbox" id="active" checked asp-for="IsPartner">
    </div>
    <input type="submit" class="btn btn-primary" value="Create" />
</form>